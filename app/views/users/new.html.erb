<h1>Sign Up to start cataloging and rating your film!</h1>

<br>

<%= render partial: 'layouts/errors', locals: {object: @user} %>

<%= form_for(@user) do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %><br>

    <%= f.label :email %>
    <%= f.email_field :email %><br>

    <%= f.label :password %>
    <%= f.password_field :password %><br>

    <%= f.submit "Sign Up!" %>

<% end %>

    